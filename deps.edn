{:paths ["src" "resources"]

 :deps
 {org.clojure/clojure                     {:mvn/version "1.12.3"}
  clojure.java-time/clojure.java-time     {:mvn/version "1.4.3"}
  com.taoensso/telemere                   {:mvn/version "1.1.0"}
  com.widdindustries/time-literals        {:mvn/version "0.1.10"}
  datascript/datascript                   {:mvn/version "1.7.6"}
  io.github.tonsky/datascript-storage-sql {:mvn/version "2.1.0"}
  io.github.tonsky/fast-edn               {:mvn/version "1.1.3"}
  org.xerial/sqlite-jdbc                  {:mvn/version "3.50.3.0"}}

 :aliases
 {:dev
  {:extra-paths ["dev"]
   :extra-deps  {io.github.tonsky/clj-reload   {:mvn/version "1.0.0"}
                 io.github.tonsky/clojure-plus {:mvn/version "1.7.0"}
                 babashka/fs                   {:mvn/version "0.5.27"}}
   :jvm-opts    ["--add-opens=java.base/java.nio=ALL-UNNAMED"
                 "--add-opens=java.base/sun.nio.ch=ALL-UNNAMED"
                 "--enable-native-access=ALL-UNNAMED"]}

  :plus
  {:exec-fn    clojure+.core.server/start-server
   :exec-args  {:name "socket repl(plus)"}}

  :nrepl
  {:extra-deps {nrepl/nrepl {:mvn/version "1.4.0"}}
   :main-opts  ["-m" "nrepl.cmdline"]}

  :kaocha {:extra-deps {lambdaisland/kaocha {:mvn/version "1.91.1392"}}
           :main-opts ["-m" "kaocha.runner"]}

  :test {:extra-paths ["test"]
         :extra-deps {org.clojure/test.check {:mvn/version "1.1.1"}
                      io.github.cognitect-labs/test-runner
                      {:git/tag "v0.5.1" :git/sha "dfb30dd"}}}

  :build {:deps {io.github.clojure/tools.build
                 {:git/tag "v0.10.10" :git/sha "deedd62"}}
          :ns-default build}

  :neil {:project {:name hkimjp/datascript-storage}}}}
